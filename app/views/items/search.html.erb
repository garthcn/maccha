<% showing = '' %>
<% pstart = (@page - 1) * @items_per_page + 1 %>
<% pend = @page * @items_per_page %>
<% pend = @count if pend > @count %>
<% if @count > 0 %>
  <% showing = "(Showing #{pstart}-#{pend})" %>
<% else %>
<% end %>
<% provide :htitle, "#{@count} results found for \"#{params[:keywords]}\" #{showing}" %>
<%= render 'grid' %>
